{
  "name": "aerowebb",
  "version": "7.1.0",
  "engines": {
    "node": "10.16.3"
  },
  "license": "unlicensed",
  "private": true,
  "scripts": {
    "start": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng serve",
    "build": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --configuration default",
    "build-aot": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --configuration aot",
    "build-prod": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --configuration prod",
    "build-staging": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --configuration staging",
    "build-daily": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --configuration daily",
    "build-optimal-daily": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --configuration dailyOptimal",
    "test": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng test",
    "lint": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng lint",
    "e2e": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng e2e",
    "stats-build-prod": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --configuration prod --stats-json",
    "stats-report": "webpack-bundle-analyzer dist/aerowebbng/stats.json",
    "new-component": "node scripts/new-component/new-component.js",
    "sonar": "env NODE_OPTIONS=--max_old_space_size=4096 SONAR_SCANNER_OPTS=-Dsonar.javascript.node.maxspace=4096 sonar-scanner"
  },
  "dependencies": {
    "@angular/animations": "7.2.12",
    "@angular/cdk": "7.3.6",
    "@angular/common": "7.2.12",
    "@angular/compiler": "7.2.12",
    "@angular/core": "7.2.12",
    "@angular/forms": "7.2.12",
    "@angular/http": "7.2.12",
    "@angular/material": "7.3.0",
    "@angular/platform-browser": "7.2.12",
    "@angular/platform-browser-dynamic": "7.2.12",
    "@angular/router": "7.2.12",
    "@ngx-translate/core": "11.0.1",
    "@ngx-translate/http-loader": "4.0.0",
    "bryntum-gantt": "file:external/gantt/build",
    "bryntum-scheduler": "file:external/scheduler/build",
    "chart.js": "2.8.0",
    "core-js": "3.0.0",
    "file-saver": "2.0.1",
    "font-awesome": "4.7.0",
    "lodash-es": "4.17.11",
    "moment": "2.24.0",
    "primeicons": "1.0.0",
    "primeng": "7.1.3",
    "rxjs": "6.5.2",
    "web-animations-js": "2.3.1",
    "xlsx": "0.14.1",
    "zone.js": "0.9.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.13.7",
    "@angular/cli": "7.3.8",
    "@angular/compiler-cli": "7.2.12",
    "@types/file-saver": "2.0.0",
    "@types/jasmine": "3.3.12",
    "@types/jasminewd2": "2.0.6",
    "@types/lodash-es": "4.17.3",
    "@types/node": "11.12.2",
    "codelyzer": "5.0.0",
    "husky": "3.1.0",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.0.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.5",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.0",
    "prettier": "1.19.1",
    "protractor": "5.4.2",
    "sonar-scanner": "^3.1.0",
    "ts-node": "8.0.3",
    "tslint": "5.14.0",
    "typescript": "3.2.4",
    "webpack-bundle-analyzer": "3.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "sh tools/format-code.sh"
    }
  }
}
